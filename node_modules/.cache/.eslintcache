ooks}) {\n  const [wishlist, setWishlist] = useState([]);\n  const [search, setSearch] = useState(\"\");\n  \n  const addToWishlistHandler = id => {\n    if (!wishlist.includes(id)) setWishlist(wishlist.concat(id));\n  };\n\n  const removeWishlistHandler = id => {\n    let indexItem = wishlist.indexOf(id);\n    let itemsBook = [...wishlist.slice(0, indexItem), ...wishlist.slice(indexItem + 1)];\n    setWishlist(itemsBook);\n  };\n \n  let searchBooks= books.filter(book => {\n    if (search === \"\") {\n      return book;\n    } else if (book.volumeInfo.title.toLowerCase().includes(search.toLowerCase())) {\n      return book;\n    }\n  });\n\n  let findWishlist = books.filter(book => wishlist.includes(book.id) );\n\n  return (\n    <div className=\"main\">\n      <div className=\"book__search\">\n        <label>Поиск</label>\n        <input\n          type=\"text\"\n          placeholder=\"Поиск\"\n          onChange={event => {\n            setSearch(event.target.value);\n          }}\n        />\n      </div>\n      <div className=\"book\">\n        <div className=\"book__list\">\n          <h2>Все книги в категории \"Пушкин\"</h2>\n          {searchBooks.map(book => {\n            return (\n              <div key={book.id} className=\"book__item\">\n                <BookDetails book= {book}/>\n                <button onClick={() => addToWishlistHandler(book.id)}>\n                  Добавить в избранное\n                </button>\n              </div>\n            );\n          })}\n        </div>\n\n        <div className=\"book__list --wishlist\">\n          <h2>Мои книги</h2>\n          {findWishlist.map(book => {\n            return (\n              <div key={book.id} className=\"book__item\">\n                <BookDetails book= {book}/>\n                <button onClick={() => removeWishlistHandler(book.id)}>\n                  Удалить\n                </button>\n              </div>\n            );\n          })}\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Book;\n","/Users/anna/Documents/react-projects/react-test/src/BookDetails.js",[],{"ruleId":"22","severity":1,"message":"23","line":4,"column":8,"nodeType":"24","messageId":"25","endLine":4,"endColumn":19},{"ruleId":"26","severity":1,"message":"27","line":18,"column":38,"nodeType":"28","messageId":"29","endLine":18,"endColumn":40},"no-unused-vars","'BookDetails' is defined but never used.","Identifier","unusedVar","array-callback-return","Array.prototype.filter() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd"]