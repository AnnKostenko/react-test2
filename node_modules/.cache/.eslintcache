l>\n        <input\n          type=\"text\"\n          placeholder=\"Поиск\"\n          onChange={event => {\n            setSearch(event.target.value);\n          }}\n        />\n      </div>\n      <div className=\"book\">\n        <div className=\"book__list\">\n          <h2>Все книги в категории \"Пушкин\"</h2>\n          {searchBooks.map(book => {\n            return (\n              <div key={book.id} className=\"book__item\">\n                <BookDetails book= {book}/>\n                <button onClick={() => addToWishlistHandler(book.id)}>\n                  Добавить в избранное\n                </button>\n              </div>\n            );\n          })}\n        </div>\n\n        <div className=\"book__list --wishlist\">\n          <h2>Мои книги</h2>\n          {findWishlist.map(book => {\n            return (\n              <div key={book.id} className=\"book__item\">\n                <BookDetails book= {book}/>\n                <button onClick={() => removeWishlistHandler(book.id)}>\n                  Удалить\n                </button>\n              </div>\n            );\n          })}\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Book;\n","/Users/anna/Documents/react-projects/react-test/src/BookDetails.js",[],"/Users/anna/Documents/react-projects/react-test-refactor/src/index.js",[],"/Users/anna/Documents/react-projects/react-test-refactor/src/BooksList.js",["39"],"/Users/anna/Documents/react-projects/react-test-refactor/src/Book.js",["40"],"/Users/anna/Documents/react-projects/react-test-refactor/src/BookDetails.js",[],{"ruleId":"41","severity":1,"message":"42","line":4,"column":8,"nodeType":"43","messageId":"44","endLine":4,"endColumn":19},{"ruleId":"45","severity":1,"message":"46","line":18,"column":38,"nodeType":"47","messageId":"48","endLine":18,"endColumn":40},{"ruleId":"41","severity":1,"message":"42","line":4,"column":8,"nodeType":"43","messageId":"44","endLine":4,"endColumn":19},{"ruleId":"45","severity":1,"message":